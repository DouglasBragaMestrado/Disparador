    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact section-bg">
      <div class="container">

        <div class="section-title" data-aos="fade-up">
          <h2>Contate-nos</h2>
        </div>

        <div class="row">

          <div class="col-lg-7 mt-5 mt-lg-0 d-flex align-items-stretch" data-aos="fade-left">
            <form asp-action="Contato" method="post" role="form" class="php-email-form">

                <div class="form-group">
                  <label for="name">@ViewBag.Name</label>
                  <input type="text" name="Nome" class="form-control" id="name" data-rule="minlen:4" data-msg="Please enter your name." />
                  <div class="validate"></div>
                </div>

                <div class="form-row">
                <div class="form-group col-md-6">
                  <label for="name">@ViewBag.Pais</label>
                   @Html.DropDownList("ddi",ViewBag.PaisSelectList as SelectList,@ViewBag.SelecionePais,new {@class="form-control"})                  
                </div>
              <div class="form-group col-md-6">
                  <label for="name">@ViewBag.Celular</label>
                   <input type="tel" maxlength="15" onkeyup="handlePhone(event)" name="phone" class="form-control" id="phone" data-rule="minlen:4" data-msg="Please enter your phone number." />
                  <div class="validate"></div>
                </div>
               </div>
               <div class="form-group">
                <label for="name">E-mail</label>
                <input type="email" class="form-control" name="Email" data-rule="email" data-msg="Please enter a valid email" />
                <div class="validate"></div>
              </div>
              <div class="form-group">
                <label for="name">@ViewBag.Assunto</label>
                <input type="text" class="form-control" name="Assunto" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                <div class="validate"></div>
              </div>
              <div class="form-group">
                <label for="name">@ViewBag.Mensagem</label>
                <textarea class="form-control" name="Mensagem" rows="10" data-rule="required" data-msg="Please write something for us"></textarea>
                <div class="validate"></div>
              </div>
              <div class="mb-3">
                <div class="loading">@ViewBag.Loading</div>
                <div class="error-message"></div>
                <div class="sent-message">@ViewBag.MensagemResposta</div>
              </div>
              <input type="hidden" name="idioma" value=@ViewBag.Idioma />
              <div class="text-center"><button type="submit">@ViewBag.EnviarMensagem</button></div>
            </form>
          </div>

        </div>

      </div>
    </section><!-- End Contact Section -->

